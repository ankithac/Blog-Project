<%-include("partials/header")-%>

<h1>Home</h1>
<p><%=homeStartingContent%></p>


<%for(var i =0;i<posts.length;i++){%>
  <h2><%=posts[i].newTitle%></h2>
  <%var postBody=posts[i].newBody;%>
  <%var postTitle=posts[i].newTitle;%>
  <%console.log(postTitle);%>
  <%var len = 100;%>
  <%var truncatedString = postBody.substring(0,len);%>
  <p><%=truncatedString%><a href="/post/<%=posts[i].newTitle%>">Read more</a></p>
<%}%>



<%-include("partials/footer")-%>
